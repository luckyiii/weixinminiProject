/* components/search/search.wxss */
.container{position: fixed;left: 0;top: 0;width: 100%;height: 90rpx;z-index: 9999;overflow: hidden;}
.containerFocus{position: fixed;left: 0;top: 0;width: 100%;height:100%;z-index: 9999;overflow: hidden;background-color: #CCC;}
.search{display: flex;align-items: center;margin: 10rpx 20rpx;}
.search-text{display: flex;align-items: center;flex: 1;border: 1px #CCC solid;border-radius: 10rpx;height: 60rpx;background-color: white;}
.search-text input{display: flex;width: 100%;}
.search-text .iconsousuo{margin: 0rpx 10rpx;}
.search-cancle{margin-left: 10rpx;}

.search-history{display: flex;align-items: center;flex: 1;justify-content: space-between;margin: 20rpx;margin-bottom: 30rpx;}
.search-history-btn text{background-color: white;border-radius: 20rpx;border: 1px #CCC solid;padding: 10rpx;margin: 10rpx;color: rgb(190, 188, 188);}
.search-history-btn{margin-bottom: 30rpx;}

.searchList-item{display: flex;height: 110rpx;border-bottom: 1px rgb(75, 73, 73) dashed;align-items: center;justify-content: space-between;}
.searchList-item view{line-height: 110rpx;display: flex;align-items: center;}
.searchList-item image{width: 100rpx;height: 100rpx;border-radius: 50%;}